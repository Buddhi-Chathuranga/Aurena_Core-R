   attr_          VARCHAR2(2000);
BEGIN
   General_SYS.Init_Method(Trn_Reservation_Item_API.lu_name_, 'Trn_Reservation_Item_API', 'Create_Reservation_Item');
   rec_.company_id := company_id_;
   rec_.reservation_id := reservation_id_;
