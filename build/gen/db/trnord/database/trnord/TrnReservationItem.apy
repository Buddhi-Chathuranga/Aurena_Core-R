      oldrec_ := Lock_By_Keys_Nowait___(remrec_.company_id, remrec_.reservation_id, remrec_.reservation_item_id);
   END IF;
   Check_Delete___(oldrec_);
   Delete___(NULL, oldrec_);
END Remove___;

