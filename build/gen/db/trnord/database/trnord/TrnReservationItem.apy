PROCEDURE Check_Common___ (
   oldrec_ IN     trn_reservation_item_tab%ROWTYPE,
   newrec_ IN OUT NOCOPY trn_reservation_item_tab%ROWTYPE,
   indrec_ IN OUT NOCOPY Indicator_Rec,
   attr_   IN OUT NOCOPY VARCHAR2 )
IS
BEGIN
   IF (newrec_.company_id IS NOT NULL AND newrec_.reservation_id IS NOT NULL)
   AND (indrec_.company_id OR indrec_.reservation_id)
   AND (Validate_SYS.Is_Changed(oldrec_.company_id, newrec_.company_id)
     OR Validate_SYS.Is_Changed(oldrec_.reservation_id, newrec_.reservation_id)) THEN
      Trn_Reservation_API.Exist(newrec_.company_id, newrec_.reservation_id);
   END IF;
   IF (newrec_.company_id IS NOT NULL AND newrec_.branch_id IS NOT NULL AND newrec_.order_id IS NOT NULL AND newrec_.row_no IS NOT NULL)
   AND (indrec_.company_id OR indrec_.branch_id OR indrec_.order_id OR indrec_.row_no)
   AND (Validate_SYS.Is_Changed(oldrec_.company_id, newrec_.company_id)
     OR Validate_SYS.Is_Changed(oldrec_.branch_id, newrec_.branch_id)
     OR Validate_SYS.Is_Changed(oldrec_.order_id, newrec_.order_id)
     OR Validate_SYS.Is_Changed(oldrec_.row_no, newrec_.row_no)) THEN
      Trn_Customer_Order_Item_API.Exist(newrec_.company_id, newrec_.branch_id, newrec_.order_id, newrec_.row_no);
   END IF;
   IF (newrec_.company_id IS NOT NULL AND newrec_.branch_id IS NOT NULL AND newrec_.inventory_id IS NOT NULL AND newrec_.product_id IS NOT NULL)
   AND (indrec_.company_id OR indrec_.branch_id OR indrec_.inventory_id OR indrec_.product_id)
   AND (Validate_SYS.Is_Changed(oldrec_.company_id, newrec_.company_id)
     OR Validate_SYS.Is_Changed(oldrec_.branch_id, newrec_.branch_id)
     OR Validate_SYS.Is_Changed(oldrec_.inventory_id, newrec_.inventory_id)
     OR Validate_SYS.Is_Changed(oldrec_.product_id, newrec_.product_id)) THEN
      Trn_Inventory_Product_API.Exist(newrec_.company_id, newrec_.branch_id, newrec_.inventory_id, newrec_.product_id);
   END IF;
   Error_SYS.Check_Not_Null(lu_name_, 'COMPANY_ID', newrec_.company_id);
   Error_SYS.Check_Not_Null(lu_name_, 'RESERVATION_ID', newrec_.reservation_id);
   Error_SYS.Check_Not_Null(lu_name_, 'RESERVATION_ITEM_ID', newrec_.reservation_item_id);
   Error_SYS.Check_Not_Null(lu_name_, 'QUANTITY', newrec_.quantity);
   Error_SYS.Check_Not_Null(lu_name_, 'BRANCH_ID', newrec_.branch_id);
   Error_SYS.Check_Not_Null(lu_name_, 'INVENTORY_ID', newrec_.inventory_id);
   Error_SYS.Check_Not_Null(lu_name_, 'PRODUCT_ID', newrec_.product_id);
   Error_SYS.Check_Not_Null(lu_name_, 'ORDER_ID', newrec_.order_id);
   Error_SYS.Check_Not_Null(lu_name_, 'ROW_NO', newrec_.row_no);
END Check_Common___;


